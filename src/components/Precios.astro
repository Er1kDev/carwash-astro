<section id="precios">
    <div class="flex flex-wrap justify-center items-center text-white">
        <div class="grid grid-cols-1 md:grid-cols-2 w-full">
            <img
                src="/public/motocicletas.webp"
                alt="motocicleta"
                class="w-full h-auto md:order-first order-last"
                data-aos="zoom-in"
            />
            <div
                class="flex flex-col justify-center items-center md:order-last order-first py-3"
                data-aos="zoom-in"
            >
                <p class="text-center lg:text-6xl md:text-4xl text-4xl">
                    Motocicletas Enduros
                </p>
                <p class="text-center lg:text-5xl md:text-3xl text-3xl">
                    $13.000
                </p>
            </div>
        </div>
    </div>
    <div class="flex flex-wrap justify-center items-center text-white">
        <div class="grid grid-cols-1 md:grid-cols-2 w-full">
            <img
                src="/public/automovil.webp"
                alt="automovil"
                class="w-full h-auto md:order-last order-last"
                data-aos="zoom-in"
            />
            <div
                class="flex flex-col justify-center items-center py-3"
                data-aos="zoom-in"
            >
                <p class="text-center lg:text-6xl md:text-4xl text-4xl">
                    Automóviles
                </p>
                <p class="text-center lg:text-5xl md:text-3xl text-3xl">
                    $18.000
                </p>
            </div>
        </div>
    </div>
    <div class="flex flex-wrap justify-center items-center text-white">
        <div class="grid grid-cols-1 md:grid-cols-2 w-full">
            <img
                src="/public/suv-jeep.webp"
                alt="sub o jeep"
                class="w-full h-full object-cover md:order-first order-last"
                data-aos="zoom-in"
            />
            <div
                class="flex flex-col justify-center items-center py-3"
                data-aos="zoom-in"
            >
                <p class="text-center lg:text-6xl md:text-4xl text-4xl">
                    Suv o Jeep
                </p>
                <p class="text-center lg:text-5xl md:text-3xl text-3xl">
                    $23.000
                </p>
            </div>
        </div>
    </div>
    <div class="flex flex-wrap justify-center items-center text-white">
        <div class="grid grid-cols-1 md:grid-cols-2 w-full">
            <img
                src="/public/jeep.webp"
                alt="camionetas"
                class="w-full h-auto md:order-last order-last"
                data-aos="zoom-in"
            />
            <div
                class="flex flex-col justify-center items-center py-3"
                data-aos="zoom-in"
            >
                <p class="text-center lg:text-6xl md:text-4xl text-4xl">
                    Camionetas
                </p>
                <p class="text-center lg:text-5xl md:text-3xl text-3xl">
                    $28.000
                </p>
            </div>
        </div>
    </div>
    <div class="flex flex-wrap justify-center items-center text-white">
        <div class="grid grid-cols-1 md:grid-cols-2 w-full">
            <img
                src="/public/camionetas.webp"
                alt="camionetasxl"
                class="w-full h-auto md:order-first order-last"
                data-aos="zoom-in"
            />
            <div
                class="flex flex-col justify-center items-center py-3"
                data-aos="zoom-in"
            >
                <p class="text-center lg:text-6xl md:text-4xl text-4xl">
                    Camionetas XL
                </p>
                <p class="text-center lg:text-5xl md:text-3xl text-3xl">
                    $33.000
                </p>
            </div>
        </div>
    </div>
</section>

